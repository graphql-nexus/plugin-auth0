    "jest": "26.2.2",
    "jest-watch-typeahead": "0.6.0",
    "nexus": "^0.26.0-next.14",
    "prettier": "2.0.5",
    "ts-jest": "26.1.4",
    "typescript": "3.9.7"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
